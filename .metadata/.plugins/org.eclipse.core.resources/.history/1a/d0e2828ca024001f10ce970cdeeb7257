package com.lgy.spring_ex2_5;

public class Multiple {
	private int num;
	
	public void process() {
		if(num % 3 == 0)
			System.out.println("3의 배수이다.");
		if(num % 3 == 0)
			System.out.println("3의 배수이다.");
		if(num % 3 == 0)
			System.out.println("3의 배수이다.");
	}
	
	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}

}
