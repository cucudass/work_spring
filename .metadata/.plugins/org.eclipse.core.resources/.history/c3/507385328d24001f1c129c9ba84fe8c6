package com.lgy.spring_2_1;

public class Calculation {
	private int fisrtNum;
	private int secondNum;
	
	public int getFisrtNum() { return fisrtNum;	}
	public int getSecondNum() { return secondNum; }
	
	public void setFisrtNum(int fisrtNum) {	this.fisrtNum = fisrtNum; }
	public void setSecondNum(int secondNum) { this.secondNum = secondNum; }
}
