package com.lgy.spring_ex2_3;

public class Three {
	private int num;
	
	public void process() {
		String str = "";
		if(num % 3 == 0)
			str = "는 3의 배수입니다.";
		else
			str = "는 3의 배수가 아닙니다.";
		System.out.println(num+str);
	}
	
	public int getNum() { return num; }

	public void setNum(int num) { this.num = num; }
	
	
}
