package com.lgy.spring_ex2_6;

public class Capital {
	private String str;
	
	public void process() {
		char ch = str.charAt(0);
		char c = 'A';
		for (int i = ch; i >= c; i--) {
			for (int j = c; j <= ch; j++) {
				System.out.print(c);
			}
			System.out.println();
		}
	}
	
	public String getStr() { return str; }

	public void setStr(String str) { this.str = str; }
}
